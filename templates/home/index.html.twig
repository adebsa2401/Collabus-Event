{% extends 'home/base.html.twig' %}

{% block body %}
  <!-- ======= Hero Section ======= -->
  <section id="hero" class="d-flex align-items-center">
    <div class="container" data-aos="zoom-out" data-aos-delay="100">
      <h1>Bienvenue à <span>CCI-Events</span>
      </h1>
      <h2>La dynamique B2B de la CCI Bénin</h2>
      <div class="d-flex">
        <a href="{{ path('app_login') }}" target="_blank" rel="noopener noreferrer" class="btn-get-started scrollto">Se connecter</a>
        <a href="{{ path('app_register') }}" target="_blank" rel="noopener noreferrer" class="btn-get-started scrollto mx-1">S'enregistrer</a>
        {#        <a href="https://www.youtube.com/watch?v=jDDaplaOz7Q" class="venobox btn-watch-video" data-vbtype="video" data-autoplay="true"> Watch Video <i class="icofont-play-alt-2"></i></a>#}
      </div>
    </div>
  </section><!-- End Hero -->

  <main id="main">

    <!-- ======= Featured Services Section ======= -->
    <section id="featured-services" class="featured-services">
      <div class="container" data-aos="fade-up">

        <div class="row">
          {% for eventType in eventTypes %}
            <div class="col-md-6 col-lg-3 d-flex align-items-stretch mb-5 mb-lg-0">
              <div class="icon-box w-100" data-aos="fade-up" data-aos-delay="400">
                <div class="icon"><i class="bx bx-world"></i></div>
                <h4 class="title"><a href="">{{ eventType.name }}</a></h4>
                <a href="{{ path('app_home_event_type_show', {'id': eventType.id}) }}" class="btn btn-primary">Evènements</a>
              </div>
            </div>
          {% endfor %}

        </div>

      </div>
    </section><!-- End Featured Services Section -->

    <!-- ======= About Section ======= -->
    <section id="about" class="about section-bg">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>B2B</h2>
          <h3>Rencontres entre <span>chefs d'entreprises</span></h3>
          <p>Mettez en place de nouveaux partenariats et dynamisez vos relations d'affaires</p>
        </div>

        <div class="row">
          <div class="col-lg-6" data-aos="fade-right" data-aos-delay="100">
            <img src="{{ asset('assets/img/about.jpeg') }}" class="img-fluid" alt="">
          </div>
          <div class="col-lg-6 pt-4 pt-lg-0 content d-flex flex-column justify-content-center" data-aos="fade-up" data-aos-delay="100">
            <h3>La CCI Bénin organise pour vous des rencontres et évènements de qualité pour vous faire profiter de son expertise</h3>
            <p class="font-italic">
              Une équipe de professionnels disponibles pour vous accompagner
            </p>
            <ul>
              <li>
                <i class="bx bx-store-alt"></i>
                <div>
                  <h5>Rencontres d'entreprises</h5>
                  <p>Des rencontres régulièrement organisées pour animer vos échanges professionnels</p>
                </div>
              </li>
              <li>
                <i class="bx bx-images"></i>
                <div>
                  <h5>Un annuaire diversifié</h5>
                  <p>Quelque soit votre secteur d'activité, retrouvez vos pairs pour de nouvelles opportunités d'affaires</p>
                </div>
              </li>
            </ul>
            <p>
              Dans les affaires humaines, une marrée montante, qu'on la saisisse au passage, elle mène à la fortune
            </p>
          </div>
        </div>

      </div>
    </section><!-- End About Section -->

    <!-- ======= Skills Section ======= -->
    <section id="skills" class="skills">
      <div class="container" data-aos="fade-up">

        <div class="row skills-content">

          <div class="col-lg-6">

            <div class="progress">
              {% set globalAttendanceRate = (globalAttendanceRate * 100)|number_format %}
              <span class="skill">Taux de présence <i class="val">{{ globalAttendanceRate }}%</i></span>
              <div class="progress-bar-wrap">
                <div class="progress-bar" role="progressbar" aria-valuenow="{{ globalAttendanceRate }}" aria-valuemin="0" aria-valuemax="100"></div>
              </div>
            </div>

            <div class="progress">
              {% set globalAttendanceValidationRate = (globalAttendanceValidationRate * 100)|number_format %}
              <span class="skill">Taux de participation <i class="val">{{ globalAttendanceValidationRate }}%</i></span>
              <div class="progress-bar-wrap">
                <div class="progress-bar" role="progressbar" aria-valuenow="{{ globalAttendanceValidationRate }}" aria-valuemin="0" aria-valuemax="100"></div>
              </div>
            </div>

            {#            <div class="progress">#}
            {#              {% set highestAttendanceRate = (highestAttendanceRate * 100)|number_format %}#}
            {#              <span class="skill">Taux d'interessement des entreprises <i class="val">{{ highestAttendanceRate }}%</i></span>#}
            {#              <div class="progress-bar-wrap">#}
            {#                <div class="progress-bar" role="progressbar" aria-valuenow="{{ highestAttendanceRate }}" aria-valuemin="0" aria-valuemax="100"></div>#}
            {#              </div>#}
            {#            </div>#}

          </div>

          <div class="col-lg-6">

            <div class="progress">
              {% set individualsInterestRate = (individualsInterestRate * 100)|number_format %}
              <span class="skill">Taux d'interessement des particuliers <i class="val">{{ individualsInterestRate }}%</i></span>
              <div class="progress-bar-wrap">
                <div class="progress-bar" role="progressbar" aria-valuenow="{{ individualsInterestRate }}" aria-valuemin="0" aria-valuemax="100"></div>
              </div>
            </div>

            <div class="progress">
              {% set companiesInterestRate = (companiesInterestRate * 100)|number_format %}
              <span class="skill">Taux d'interessement des entreprises <i class="val">{{ companiesInterestRate }}%</i></span>
              <div class="progress-bar-wrap">
                <div class="progress-bar" role="progressbar" aria-valuenow="{{ companiesInterestRate }}" aria-valuemin="0" aria-valuemax="100"></div>
              </div>
            </div>

            {#            <div class="progress">#}
            {#              <span class="skill">Photoshop <i class="val">55%</i></span>#}
            {#              <div class="progress-bar-wrap">#}
            {#                <div class="progress-bar" role="progressbar" aria-valuenow="55" aria-valuemin="0" aria-valuemax="100"></div>#}
            {#              </div>#}
            {#            </div>#}

          </div>

        </div>

      </div>
    </section><!-- End Skills Section -->

    <!-- ======= Counts Section ======= -->
    <section id="counts" class="counts">
      <div class="container" data-aos="fade-up">

        <div class="row">

          <div class="col-lg-3 col-md-6">
            <div class="count-box">
              <i class="icofont-bank-alt"></i>
              <span data-toggle="counter-up">{{ companies|length|number_format }}</span>
              <p>Entreprises</p>
            </div>
          </div>

          <div class="col-lg-3 col-md-6 mt-5 mt-md-0">
            <div class="count-box">
              <i class="icofont-addons"></i>
              <span data-toggle="counter-up">{{ activityAreas|length|number_format }}</span>
              <p>Secteurs d'Activité</p>
            </div>
          </div>

          <div class="col-lg-3 col-md-6 mt-5 mt-lg-0">
            <div class="count-box">
              <i class="icofont-meeting-add"></i>
              <span data-toggle="counter-up">{{ allEvents|length|number_format }}</span>
              <p>Evènements</p>
            </div>
          </div>

          <div class="col-lg-3 col-md-6 mt-5 mt-lg-0">
            <div class="count-box">
              <i class="icofont-meeting-add"></i>
              <span data-toggle="counter-up">{{ events|length|number_format }}</span>
              <p>Evènements en cours</p>
            </div>
          </div>

        </div>

      </div>
    </section><!-- End Counts Section -->

    <!-- ======= Clients Section ======= -->
    <section id="clients" class="clients section-bg">
      <div class="container" data-aos="zoom-in">

        <div class="row">

          {% for company in companies %}
            <div class="col-lg-2 col-md-4 col-6 d-flex align-items-center justify-content-center">
              <img src="{{ company.logo ? asset('uploads/company_logos/' ~ company.logo) : asset('assets/img/clients/client-default.png') }}" class="img-fluid" alt="">
            </div>
          {% endfor %}

        </div>

      </div>
    </section><!-- End Clients Section -->

    <!-- ======= Services Section ======= -->
    <section id="services" class="services portfolio">
      <div class="container" data-aos="zoom-in">

        <div class="section-title">
          <h2>Evènements</h2>
          <h3><span>Nos évènements</span></h3>
          <p>Visitez nos évènements à venir, en cours ou passés et consultez leurs statistiques en temps réel</p>
        </div>

        <div class="row" data-aos="zoom-in" data-aos-delay="100">
          <div class="col-lg-12 d-flex justify-content-center">
            <ul id="portfolio-flters" class="portfolio-flters">
              <li data-filter="*" class="filter-active">All</li>
              {% for eventType in eventTypes %}
                <li data-filter=".filter-event-{{ eventType.name|u.snake }}">{{ eventType.name }}</li>
              {% endfor %}
            </ul>
          </div>
        </div>

        <div class="row portfolio-container">
          {% for event in allEvents %}
            <a href="{{ path('app_home_event_show', {'id': event.id}) }}" class="col-lg-4 col-md-6 portfolio-item filter-event-{{ event.type.name|u.snake }}" data-aos="zoom-in" data-aos-delay="100">
              <div class="icon-box p-0">
                <div class="member-img">
                  <img src="{{ asset('uploads/events_images/' ~ event.imageUrl) }}" class="img-fluid" style="aspect-ratio: 16/9" alt="">
                </div>
                <h4 class="m-5">{{ event.name }}</h4>
              </div>
            </a>
          {% endfor %}
        </div>

      </div>
    </section><!-- End Services Section -->

