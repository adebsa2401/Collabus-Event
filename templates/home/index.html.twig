              <div class="box">
                <span class="advanced" style="background-color: {{ event.type.color }}">{{ event.type.name }}</span>
                <h3>{{ event.name }}</h3>
                <h4><sup>{{ event.startedAt|date('d/m/y') }} - {{ event.endedAt|date('d/m/y') }}</sup></h4>
                <ul>
                  <li>{{ event.place.name }}</li>
                  <li>{{ event.place.standCount|number_format }} places debout</li>
                  <li>{{ event.place.sitCount|number_format }} places assises</li>
                  <li>{{ event.participationRequests|length|number_format }} requêtes de participation</li>
                </ul>
                <div class="btn-wrap">
                  <a href="{{ path('app_event_participate', {'id': event.id}) }}" class="btn-buy">Participer</a>
                </div>
              </div>
            </div>
          {% endfor %}

        </div>

      </div>
    </section><!-- End Pricing Section -->

