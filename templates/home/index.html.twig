    <section id="pricing" class="pricing portfolio">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>Participations</h2>
          <h3>Participer à nos <span>Evènements</span></h3>
          {% if events|length == 0 %}
            <p>Aucun évènement à venir</p>
          {% endif %}
        </div>

        <div class="row" data-aos="fade-up" data-aos-delay="100">
          <div class="col-lg-12 d-flex justify-content-center">
            <ul id="portfolio-flters" class="portfolio-flters">
              <li data-filter="*" class="filter-active">All</li>
              {% for eventType in eventTypes %}
                <li data-filter=".filter-attendance-{{ eventType.name|u.snake }}">{{ eventType.name }}</li>
              {% endfor %}
            </ul>
          </div>
        </div>

        <div class="row portfolio-container">

          {% for event in events %}
